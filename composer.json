{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "misaf/vendra-faq-api",
    "description": "API services for the vendra-faq module",
    "type": "vendra-module",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filamentphp",
        "misaf",
        "vendra",
        "faq-api"
    ],
    "homepage": "https://github.com/misaf/vendra-faq-api",
    "support": {
        "issues": "https://github.com/misaf/vendra-faq-api/issues",
        "source": "https://github.com/misaf/vendra-faq-api"
    },
    "authors": [
        {
            "name": "Ehsan Mahmoodi",
            "email": "misaf.1990@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "misaf/vendra-faq": "dev-master",
        "laravel-json-api/laravel": "^5.1",
        "spatie/laravel-package-tools": "^1.92"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24.1",
        "larastan/larastan": "^3.9.2",
        "laravel/boost": "^2.0.6",
        "laravel/pint": "^1.27",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.3",
        "orchestra/testbench": "^10.9",
        "pestphp/pest": "^4.3.2",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-profanity": "^4.2.1",
        "pestphp/pest-plugin-type-coverage": "^4.0.3"
    },
    "autoload": {
        "psr-4": {
            "Misaf\\VendraFaqApi\\": "src/",
            "Misaf\\VendraFaqApi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Misaf\\VendraFaqApi\\FaqApiServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../*",
            "options": {
                "symlink": true
            }
        }
    ]
}
